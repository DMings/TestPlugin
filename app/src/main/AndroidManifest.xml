<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools" package="com.dming.testplugin">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:name=".MyApp"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme" tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.dming.testplugin.PluginActivityS1"/>
        <activity android:name="com.dming.testplugin.PluginActivityS2"/>
        <activity android:name="com.dming.testplugin.PluginActivityS3"/>
        <activity android:name="com.dming.testplugin.PluginActivityS4"/>
        <activity android:name="com.dming.testplugin.PluginActivityS5"/>
        <activity android:name="com.dming.testplugin.PluginActivityS6"/>
        <activity android:name="com.dming.testplugin.PluginActivityS7"/>
        <activity android:name="com.dming.testplugin.PluginActivityS8"/>
        <service android:name=".MyService"/>
    </application>

</manifest>